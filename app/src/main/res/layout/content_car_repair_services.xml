<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_gravity="center"
    android:gravity="center"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:id="@+id/tyrelayout"
                android:layout_weight="1"
                android:layout_marginLeft="20dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <ImageView
                android:id="@+id/tyrechargingcheck"
                android:background="@drawable/greentickmark"
                android:layout_width="16dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:visibility="gone"
                android:layout_marginTop="10dp"
                android:layout_height="16dp" />
            <LinearLayout
                android:orientation="vertical"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="90dp">
                <ImageView
                    android:id="@+id/tyreimage"
                    android:layout_gravity="center"
                    android:background="@drawable/tyrechargingicon"
                    android:layout_width="60dp"
                    android:layout_height="50dp" />
                <TextView
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="@string/tyrecharging"
                    android:textStyle="bold"
                    android:textSize="12dp"
                    android:layout_marginTop="4dp"
                    android:textColor="@color/white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            </RelativeLayout>
            <View
                android:background="@color/white"
                android:layout_width="1dp"
                android:layout_height="90dp"/>
            <RelativeLayout
                android:id="@+id/towlinglayout"
                android:layout_weight="1"
                android:layout_marginRight="20dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <ImageView
                android:id="@+id/vechicletowingcheck"
                android:background="@drawable/greentickmark"
                android:layout_width="16dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:visibility="gone"
                android:layout_marginTop="10dp"
                android:layout_height="16dp" />
            <LinearLayout
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/towingimage"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@drawable/vehicletowingicon"
                    android:layout_width="60dp"
                    android:layout_height="50dp" />
                <TextView
                    android:text="@string/vechicletowling"
                    android:textStyle="bold"
                    android:textSize="12dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_marginTop="4dp"
                    android:textColor="@color/white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
            </LinearLayout>
       </RelativeLayout>
        </LinearLayout>
        <View
            android:background="@color/white"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_width="match_parent"
            android:layout_height="1dp"/>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:id="@+id/carginglayout"
                android:layout_weight="1"
                android:layout_marginLeft="20dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <ImageView
                android:id="@+id/batterychargingcheck"
                android:background="@drawable/greentickmark"
                android:layout_width="16dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:visibility="gone"
                android:layout_marginTop="10dp"
                android:layout_height="16dp" />
            <LinearLayout
                android:orientation="vertical"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="90dp">
                <ImageView
                    android:id="@+id/chargingimage"
                    android:layout_gravity="center"
                    android:background="@drawable/batterychargingicon"
                    android:layout_width="60dp"
                    android:layout_height="50dp" />
                <TextView
                    android:text="@string/batterycharging"
                    android:textStyle="bold"
                    android:textSize="12dp"
                    android:layout_marginTop="4dp"
                    android:textColor="@color/white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            </RelativeLayout>
            <View
                android:background="@color/white"
                android:layout_width="1dp"
                android:layout_height="90dp"/>
            <RelativeLayout
                android:id="@+id/partsreparinglayout"
                android:layout_weight="1"
                android:layout_marginRight="20dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <ImageView
                android:id="@+id/partsrepaircheck"
                android:background="@drawable/greentickmark"
                android:layout_width="16dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:visibility="gone"
                android:layout_marginTop="10dp"
                android:layout_height="16dp" />
            <LinearLayout
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/repairingimage"
                    android:layout_gravity="center"
                    android:background="@drawable/mechanicalpartsrepairingicon"
                    android:layout_width="60dp"
                    android:layout_height="50dp" />
                <TextView
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="@string/mechnicalparts"
                    android:textStyle="bold"
                    android:textSize="12dp"
                    android:layout_marginTop="4dp"
                    android:textColor="@color/white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
           </RelativeLayout>
        </LinearLayout>
        <View
            android:background="@color/white"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_width="match_parent"
            android:layout_height="1dp"/>
        <LinearLayout
            android:layout_marginBottom="40dp"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:id="@+id/partsreplacementlayout"
                android:layout_weight="1"
                android:layout_marginLeft="20dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <ImageView
                android:id="@+id/bodypartsreplacementcheck"
                android:background="@drawable/greentickmark"
                android:layout_width="16dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:visibility="gone"
                android:layout_marginTop="10dp"
                android:layout_height="16dp" />
            <LinearLayout
                android:orientation="vertical"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/bodyrepairimage"
                    android:layout_gravity="center"
                    android:background="@drawable/bodypartsreplacement"
                    android:layout_width="60dp"
                    android:layout_height="50dp" />
                <TextView
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="@string/bodyparts"
                    android:textStyle="bold"
                    android:textSize="12dp"
                    android:layout_marginTop="4dp"
                    android:textColor="@color/white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            </RelativeLayout>
            <View
                android:background="@color/white"
                android:layout_width="1dp"
                android:layout_height="90dp"/>
            <RelativeLayout
                android:id="@+id/carservicelayout"
                android:layout_weight="1"
                android:layout_marginRight="20dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <ImageView
                android:id="@+id/carservicecheck"
                android:background="@drawable/greentickmark"
                android:layout_width="16dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:visibility="gone"
                android:layout_marginTop="10dp"
                android:layout_height="16dp" />
            <LinearLayout
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/serviceimage"
                        android:layout_gravity="center"
                        android:background="@drawable/carservicesicon"
                        android:layout_width="60dp"
                        android:layout_height="50dp" />
                    <TextView
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="@string/carservices"
                        android:textStyle="bold"
                        android:textSize="12dp"
                        android:layout_marginTop="4dp"
                        android:textColor="@color/white"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
            </LinearLayout>
          </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </LinearLayout>


    </LinearLayout>

</LinearLayout>